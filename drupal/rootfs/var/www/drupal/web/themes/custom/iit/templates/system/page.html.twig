        {% endif %}

        {% if page.search or page.secondary_nav or page.primary_nav or page.header_cta_nav %}
          <div class="site-header__main">
            <div class="site-header__main__inner">
              {{ page.search }}

              {% if page.secondary_nav %}
                <div class="site-header__top">
                  <div class="site-header__top__inner">
                    {{ page.secondary_nav }}
                  </div>
                </div>
              {% endif %}

              {% if page.primary_nav %}
                <div class="site-header__bottom">
                  {{ page.primary_nav }}
                </div>
              {% endif %}

              {% if page.header_cta_nav %}
                <div class="header-btns header-btns--mobile">
                  {{ page.header_cta_nav }}
                </div>
              {% endif %}
            </div>
          </div>
        {% endif %}

      </div>
    </div>
  </header>

