{% if q is not empty %}
{{ q }}
{% endif %}

<div class='grid-x grid-margin-x'>
  <div class='cell initial-12 medium-4 columns'>
    <div class="listing__form__submit-container">
      {{ form.search }}
      {{ form.actions }}
    </div>
  </div>
  <div class='cell initial-12 medium-4 columns'>
    {{ form.subject }}
  </div>
  {{ form|without('search', 'subject', 'actions') }}
</div>
